# 3D arrow model for AR navigation
# Designed to match Google Maps AR style

# Vertices for the main arrow body
v 0.0 0.0 -0.5    # Base center
v 0.3 0.0 -0.5    # Base right
v 0.3 0.0 0.3     # Middle right
v 0.6 0.0 0.3     # Arrow head right
v 0.0 0.0 1.0     # Arrow tip
v -0.6 0.0 0.3    # Arrow head left
v -0.3 0.0 0.3    # Middle left
v -0.3 0.0 -0.5   # Base left

# Vertices for 3D effect (top layer)
v 0.0 0.1 -0.5    # Top base center
v 0.3 0.1 -0.5    # Top base right
v 0.3 0.1 0.3     # Top middle right
v 0.6 0.1 0.3     # Top arrow head right
v 0.0 0.1 1.0     # Top arrow tip
v -0.6 0.1 0.3    # Top arrow head left
v -0.3 0.1 0.3    # Top middle left
v -0.3 0.1 -0.5   # Top base left

# Texture coordinates
vt 0.0 0.0
vt 1.0 0.0
vt 1.0 1.0
vt 0.0 1.0

# Normals
vn 0.0 1.0 0.0    # Up
vn 0.0 -1.0 0.0   # Down
vn 0.0 0.0 1.0    # Forward
vn 1.0 0.0 0.0    # Right
vn -1.0 0.0 0.0   # Left

# Bottom face
f 1/1/2 2/2/2 3/3/2 
f 1/1/2 3/3/2 7/4/2
f 7/1/2 8/2/2 1/3/2
f 3/1/2 4/2/2 5/3/2
f 5/1/2 6/2/2 7/3/2
f 3/1/2 5/2/2 7/3/2

# Top face
f 9/1/1 11/2/1 10/3/1
f 9/1/1 15/2/1 11/3/1
f 15/1/1 9/2/1 16/3/1
f 11/1/1 12/2/1 13/3/1
f 13/1/1 14/2/1 15/3/1
f 11/1/1 13/2/1 15/3/1

# Side faces
f 1/1/3 9/2/3 10/3/3 2/4/3    # Back
f 2/1/4 10/2/4 11/3/4 3/4/4    # Right side
f 3/1/4 11/2/4 12/3/4 4/4/4    # Right arrow
f 4/1/3 12/2/3 13/3/3 5/4/3    # Right tip
f 5/1/3 13/2/3 14/3/3 6/4/3    # Left tip
f 6/1/5 14/2/5 15/3/5 7/4/5    # Left arrow
f 7/1/5 15/2/5 16/3/5 8/4/5    # Left side
f 8/1/3 16/2/3 9/3/3 1/4/3     # Back